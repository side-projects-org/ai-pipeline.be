variable projectName {
  description = "identifier of resource group"
  type        = string
  default     = "not-assigned"
}

variable stage {
  description = "Stage of resource group"
  type        = string
  default     = "not-assigned"
}

variable awsAccountId {
  description = "aws account id"
  type        = string
  default     = "not-assigned"
}

variable awsRegion {
  description = "aws region"
  type        = string
  default     = "not-assigned"
}




